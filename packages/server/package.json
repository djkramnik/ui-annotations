{
  "name": "ui-labelling-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "prisma:init": "npx prisma init",
    "prisma:generate": "npx prisma generate --schema ../annotation_schema/prisma/schema.prisma",
    "prisma:migrate:dev": "npx prisma migrate dev --schema ../annotation_schema/prisma/schema.prisma --name",
    "prisma:migrate:deploy": "npx prisma migrate deploy --schema ../annotation_schema/prisma/schema.prisma",
    "build": "tsc && cp .env ./dist/.env",
    "watch": "tsc -w"
  },
