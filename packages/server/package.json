  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.14",
    "dotenv": "^16.4.7",
    "nodemon": "^3.1.10",
    "prisma": "^6.9.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "annotation_schema": "^1.0.0",
    "express": "^5.1.0",
    "sharp": "^0.34.3",
    "ui-labelling-shared": "^1.1.0"
  }
