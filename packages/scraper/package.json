{
  "name": "scraper",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "npm run build && node dist/index.js",
    "start:synth": "npm run build && node dist/index.js --processor=synth --transform=synth --links=synth --max_scroll=0",
    "start:synth:debug": "npm run build && node dist/index.js --processor=synth --transform=synth --links=synth --debug=true --max_scroll=0",
    "start:video-synth": "npm run build && node dist/index.js --headless=n --processor=video-synth --transform=video-synth --links=video-synth --max_scroll=0",
    "start:ad": "npm run build && node dist/index.js --headless=n --processor=ads --transform=ads --links=ads --max_scroll=0",
    "start:img": "npm run build && node dist/index.js --headless=n --processor=image-scrape --transform=image-scrape --links=image-scrape --max_scroll=0",
    "prisma:db:pull": "npx prisma db pull --schema ../annotation_schema/prisma/schema.prisma"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "dotenv": "^17.2.3",
    "prisma": "^6.14.0",
    "typescript": "^5.9.2",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "annotation_schema": "^1.0.0",
    "image-size": "^2.0.2",
    "puppeteer-core": "^24.19.0",
    "sharp": "^0.34.4",
    "ui-labelling-shared": "^1.1.0",
    "zod": "^4.2.1"
  }
}
