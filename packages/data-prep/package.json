{
  "name": "data-prep",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "prep-ocr": "npm run build && node dist/prep_ocr.js",
    "ocr": "npm run build && node dist/write_ocr.js",
    "start": "npm run build && node dist/index.js",
    "migrate_annos": "npm run build && node dist/migrate_annotations.js",
    "unwrap_annotations": "npm run build && node dist/unwrap_annos.js",
    "ocr_annotations": "npm run build && node dist/ocr_annotations.js",
    "fix_ids": "npm run build && node dist/fix_anno_ids.js",
    "write_crops": "npm run build && node dist/write_img_crops.js",
    "write_interactive": "npm run build && node dist/write_interactive.js",
    "start:textregion": "npm run build && CLASS_NAMES=textRegion OUTPUT_ROOT=coco_textregion TAG=textregion node dist/index.js",
    "start:interactive": "npm run build && CLASS_NAMES=interactive OUTPUT_ROOT=coco_interactive TAG=interactive node dist/index.js",
    "prisma:db:pull": "npx prisma db pull --schema ../annotation_schema/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "prisma": "^6.14.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "annotation_schema": "^1.0.0",
    "sharp": "^0.34.4",
    "ui-labelling-shared": "^1.1.0"
  }
}
